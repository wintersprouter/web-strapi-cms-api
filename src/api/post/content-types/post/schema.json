{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "slug": {
      "type": "uid",
      "targetField": "title",
      "unique": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "datetime"
    },
    "description": {
      "type": "text"
    },
    "content": {
      "type": "richtext"
    },
    "author": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false
    },
    "subcategory": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::subcategory.subcategory",
      "inversedBy": "posts"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "posts"
    },
    "keywords": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::keyword.keyword",
      "inversedBy": "posts"
    }
  }
}
